CREATE TABLE doulist (
  id INTEGER PRIMARY KEY ,
  movieid varchar(16) ,
  doulist_url varchar(128),
  doulist_name varchar(128) ,
  doulist_intr varchar(1024) ,
  user_name varchar(32) ,
  user_url varchar(128),
  collect_num INTEGER ,
  recommend_num INTEGER,
  movie_num INTEGER,
  doulist_cratedDate date ,
  doulist_updatedDate date ,
  created_time date ,
  updated_time varchar(32)
) SALT_BUCKETS=3;

CREATE TABLE steaming_record (
  id varchar(64) PRIMARY KEY ,
  time varchar(32),
  recordCount BIGINT,
  recordType varchar(16),
  batchRecordId varchar(32),
  created_time date ,
  updated_time varchar(32)
) SALT_BUCKETS=3;

case class SteamingRecord(id: String, time: String, recordUpdateCount: Long, recordType: String,
                          batchRecordId: String)


